<% genres.each do |genre| %>
  <tr>
  	<td><%= genre.name %></td>
  	<td>
      <% if genre.is_active == true %>
        有効
      <% else %>
        無効
      <% end %>
	</td>
	<td class="btn-hover">
	  <%= link_to "編集する",edit_admins_genre_path(genre),class: "btn btn-primary btn-sm" %>
	</td>
  </tr>
<% end %>