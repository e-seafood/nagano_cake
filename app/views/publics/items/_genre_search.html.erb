<table class="table table-bordered" id="table-borderless">
  <tr>
    <th>ジャンル検索</th>
  </tr>
    <% genres.each do |genre| %>
      <tr>
        <td>
          <%= link_to items_path(id: genre.id) do %>
            <%= genre.name %>
          <% end %>
        </td>
      </tr>
    <% end %>
</table>