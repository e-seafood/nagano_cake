<table class="table">
  <tbody class="table-hover">
  <tr>
          <th class="active">ジャンル検索</th>
  </tr>
    <% genres.each do |genre| %>
      <tr>
        <td class="btn-hover">
          <%= link_to items_path(id: genre.id) do %>
            <%= genre.name %>
          <% end %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>